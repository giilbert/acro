entities:
  - name: Mesh
    components:
      - name: Transform
        position: [0.0, 0.0, 0.0]
        rotation: [0.0, 0.0, 0.0]
        scale: [1.0, 1.0, 1.0]
      - name: Mesh
        geometry:
          type: Embedded
          vertices:
            - position: [-1.0, -1.0, 0.0]
              tex_coords: [0.0, 1.0]
            - position: [1.0, -1.0, 0.0]
              tex_coords: [1.0, 1.0]
            - position: [1.0, 1.0, 0.0]
              tex_coords: [1.0, 0.0]
            - position: [-1.0, 1.0, 0.0]
              tex_coords: [0.0, 0.0]
          indices: [0, 1, 2, 0, 2, 3]
        diffuse_texture: examples/simple/textures/ferris.png
        shader_path: examples/simple/shaders/basic-mesh.wgsl
      - name: Behavior
        source: examples/simple/src/test.ts
    children: []
  - name: Camera
    components:
      - name: Transform
        position: [0.0, 0.0, -20.0]
        rotation: [0.0, 0.0, 0.0]
        scale: [1.0, 1.0, 1.0]
      - name: Camera
        is_main_camera: true
        type: Perspective
        fov: 70.0
        near: 0.01
        far: 1000.0
      - name: Behavior
        source: examples/simple/src/fly-camera.ts
    children: []
  - name: UI
    components:
      - name: Transform
        position: [0.0, 0.0, 0.0]
        rotation: [0.0, 0.0, 0.0]
        scale: [1.0, 1.0, 1.0]
      - name: ScreenUi
    children: []
