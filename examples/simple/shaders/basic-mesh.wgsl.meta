diffuse_texture: textures/ferris.png
bind_groups:
  - id: ModelMatrix
    uniforms:
      - id: ModelMatrix
        stage: 1
        uniform_type: Mat4
  - id: ViewProjectionMatrix
    uniforms:
      - id: ViewMatrix
        stage: 1
        uniform_type: Mat4
      - id: ProjectionMatrix
        stage: 1
        uniform_type: Mat4
  - id: DiffuseTexture
    uniforms:
      - id: Texture2D
        stage: 2
        uniform_type: Texture2D
      - id: Sampler
        stage: 2
        uniform_type: Sampler
